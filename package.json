{
   "name": "@sheetbase/core-server",
   "version": "0.0.20",
   "description": "Sheetbase core module for backend app.",
   "author": "Sheetbase",
   "homepage": "https://sheetbase.net",
   "license": "MIT",
   "files": [
      "dist"
   ],
   "main": "dist/bundles/sheetbase-core-server.umd.js",
   "module": "dist/fesm3/sheetbase-core-server.js",
   "typings": "dist/sheetbase-core-server.d.ts",
   "scripts": {
      "module": "sheetbase-app-scripts build --module",
      "readme": "sheetbase-app-scripts readme",
      "docs": "sheetbase-app-scripts docs",
      "build": "npm run module && npm run readme && npm run docs",
      "lint": "tslint --project tslint.json && echo 'All files pass linting.'",
      "test": "tsc --project tests/tsconfig.test.json && nyc --cache false mocha --timeout 100000 -- build/tests/*.js",
      "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
   },
   "keywords": [
      "google-apps-script",
      "sheetbase",
      "webapp",
      "api",
      "expressjs"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/sheetbase/core-server.git"
   },
   "bugs": {
      "url": "https://github.com/sheetbase/core-server/issues"
   },
   "dependencies": {},
   "devDependencies": {
      "@sheetbase/app-scripts": "0.0.30",
      "@types/chai": "^4.1.7",
      "@types/google-apps-script": "^0.0.32",
      "@types/mocha": "^5.2.5",
      "@types/node": "^10.12.8",
      "chai": "^4.2.0",
      "coveralls": "^3.0.2",
      "mocha": "^5.2.0",
      "nyc": "^13.1.0",
      "tslint": "^5.11.0",
      "typescript": "^3.1.6"
   }
}
